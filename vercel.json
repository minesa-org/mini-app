{
	"buildCommand": "npm run build",
	"functions": {
		"api/interactions.ts": {
			"maxDuration": 10,
			"includeFiles": "dist/**"
		},
		"api/discord-oauth-callback.ts": {
			"maxDuration": 10,
			"includeFiles": "dist/**"
		},
		"api/get-oauth-url.ts": {
			"maxDuration": 10,
			"includeFiles": "dist/**"
		}
	},
	"rewrites": [
		{
			"source": "/",
			"destination": "/public/index.html"
		}
	]
}